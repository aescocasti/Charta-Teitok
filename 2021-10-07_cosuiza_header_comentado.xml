         <sourceDesc> <!-- Contiene la información relativa a la fuente de la edición digital. Obligatorio-->
            <msDesc><!-- Contiene la descripción de un manuscrito-->
               <msIdentifier><!-- Contiene la información que permite identificar el manuscrito fuente-->
                  <country>Suiza</country>
                  <region type="canton"></region> <!-- Región. Type cantón en el caso de Suiza-->
                  <settlement></settlement> <!-- Ciudad, comuna-->
                  <repository></repository> <!-- Biblioteca o archivo-->
                  <idno></idno> <!-- Identificador, fondo, signatura-->
               </msIdentifier>
               <msContents> <!-- Contiene información relativa al contenido de un manuscrito-->
                  <summary></summary> <!-- Resumen-->
                  <msItem class="original"><!-- Esta class para la mayoría de los documentos. También podría ser copia-->
                     <editor>
                        <persName type="escribano/notario"></persName> <!-- Nombre del scriptor (escribano, notario)-->
                     </editor>
                  </msItem>
               </msContents>
               <physDesc><!-- Descripción física del manuscrito-->
                  <objectDesc form="ms"><!-- Tipo de objeto (ms, letter)-->
                     <supportDesc material="paper"><!-- Material-->
                        <extent><!-- Dimensión del manuscrito-->
                           <note></note> <!-- En prosa: cantidad de hojas-->
                           <dimensions scope="all" unit="mm"><!-- Unidad de medida-->
                              <height></height> <!-- Altura-->
                              <width></width> <!-- Ancho-->
                           </dimensions>
                        </extent>
                        <condition><p></p></condition> <!-- Estado de conservación en elemento <p>-->
                     </supportDesc>
                  </objectDesc>
                  <handDesc hands="1"> <!-- Número de manos-->
                     <handNote resp="#h1" />  <!-- resp para el identificador de mano-->
                  </handDesc>
                  <bindingDesc> <!-- Descripción encuadernación-->
                     <binding><p></p></binding> <!-- Información relativa a la encuadernación en elemento <p>-->
                  </bindingDesc>
               </physDesc>
               <history> <!-- Contiene elementos que describen la historia de un manuscrito-->
                  <origin> <!-- Información relativa a su origen-->
                     <origDate when="aaaa" type="explicit"></origDate> <!-- Fecha: when="año" y contenido del elemento con fecha completa: 1342 diciembre 23-->
                     <origPlace type="explicit">
                        <placeName type="settlement"></placeName> <!-- Ciudad-->
                        <region type="region"></region> <!-- Región-->
                        <country></country> <!-- País-->
                        <geo></geo> <!-- Coordenadas: lat-long-->
                     </origPlace>
                  </origin>
               </history>
            </msDesc>
         </sourceDesc>
